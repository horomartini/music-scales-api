{
  "name": "music-scales-grpc",
  "version": "1.0.0",
  "main": "main.jsx",
  "scripts": {
    "build": "tsc -p .",
    "dev": "nodemon src/main.ts",
    "test:client": "node --nolazy -r ts-node/register -r tsconfig-paths/register src/test/client.ts",
    "protoc": "protoc --proto_path=./src/proto --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/proto/__generated__/ --ts_proto_opt=outputServices=grpc-js ./src/proto/*.proto",
    "protoc:linux": "npm run protoc -- --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto && cp ./src/proto/__generated__/*.ts ../shared/src/proto/__generated__/",
    "protoc:windows": "npm run protoc -- --plugin=protoc-gen-ts_proto=\".\\node_modules\\.bin\\protoc-gen-ts_proto.cmd\" && copy .\\src\\proto\\__generated__\\*.ts ..\\apollo\\src\\proto\\__generated__\\*.ts /Y && copy .\\src\\proto\\__generated__\\*.ts ..\\rest\\src\\proto\\__generated__\\*.ts /Y"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@grpc/grpc-js": "^1.12.5",
    "mongoose": "^8.9.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "ts-proto": "^2.6.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  }
}
